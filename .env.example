# Orchids-2api 环境变量配置
# 复制此文件为 .env 并填写实际值

# ============ 服务配置 ============
PORT=3002
DEBUG_ENABLED=true

# ============ 管理员配置 ============
# 建议在生产环境中修改这些值
ADMIN_USER=admin
ADMIN_PASS=your_secure_password_here
ADMIN_PATH=/your_secret_admin_path

# ============ Clerk 认证配置 ============
# 这些值需要从 Orchids 账号获取
SESSION_ID=
CLIENT_COOKIE=
CLIENT_UAT=
PROJECT_ID=
USER_ID=
EMAIL=

# ============ 默认模型 ============
AGENT_MODE=claude-opus-4.5
